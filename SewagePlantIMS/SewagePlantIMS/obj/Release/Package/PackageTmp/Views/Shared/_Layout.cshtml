<script>
    function swal_success() {
        swal("", "操作成功!", "success", {
            buttons: false,
            timer: 1600,
        });
    }
    function swal_error() {
        swal("", "操作失败!", "error", {
            buttons: false,
            timer: 1600,
        });
    }
    function jump_electrical_list() {

        window.setTimeout("window.location='Electrical_List'", 2000);
    }
    function jump_ChemicalDeviceList() {

        window.setTimeout("window.location='ChemicalDeviceList'", 2000);
    }
    function jump_DeviceList() {
        window.setTimeout("window.location='DeviceList'", 2000);
    }
</script>

